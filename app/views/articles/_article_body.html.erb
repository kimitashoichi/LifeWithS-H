  <div class="wrap">
  	 	<ul id="tab-menu" class="clearfix">
  	 		<li><a href="#tab1" class="active">新着一覧</a></li>
  	 		<li><a href="#tab2" class="">急上昇</a></li>
  	 	</ul>

	 <div id="tab-contents" class="clearfix">
	 <!--新着記事一覧表示  -->
		  <div id="tab1" class="tab">
		  	<div class="container-fluid">
			  <% article.each do |practice| %>
				 <%= link_to article_path(practice.id) do %>
					<div class="row article-box ">
						<div class="col-md-5 skate-image">
						  <%= attachment_image_tag practice.article_images.last, :image, :size=>"200x200" %>
						</div>
						<div class="col-md-7 skate-article-box">
						  <h3 class="skate-article-title"><%= practice.article_title %></h3>
						   <p class="skate-article-created"><%=practice.created_at.strftime("%Y.%m.%d. %H:%M") %></p>
						</div>
					</div>
				 <% end %>
			  <% end %>
			</div>
			    <div class="paginate">
                  <%= paginate article, class: "paginate" %>
               </div>
		  </div>

  <!-- 閲覧数ランキング一覧表示 -->
		  <div id="tab2" class="tab">
            <%  if browse_rank.present?  %>
             <div class="container-fluid">
			  <% browse_rank.each do |practice| %>
				 <%= link_to article_path(practice.id) do %>
					<div class="row article-box ">
						<div class="col-md-5 skate-image">
						  <%= attachment_image_tag practice.article_images.last, :image, :size=>"200x200" %>
						</div>
						<div class="col-md-7 skate-article-box">
						  <h3 class="skate-article-title"><%= practice.article_title %></h3>
						  <p class="skate-article-created"><%= practice.created_at.strftime("%Y.%m.%d. %H:%M") %></p>
						</div>
					</div>
				 <% end %>
			  <% end %>
			</div>
			  	<div class="paginate">
                  <%= paginate browse_rank, class: "paginate" %>
               </div>

        <% else %>
              <h1 class="article-none-message">急上昇記事はまだありません</h1>
	        <% end %>
		  </div>
     </div>
  </div>